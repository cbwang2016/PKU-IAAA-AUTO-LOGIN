{
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
       "js" : [ "iaaa.js" ],
       "matches": [ "https://iaaa.pku.edu.cn/*"]
   } ],
   "description": "Automatic PKU-IAAA login. Just you need to save it once in the Popup and leave it on the extension.",
   "icons": {
      "128": "icon128.png",
      "16": "icon.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "PKU IAAA Auto Login",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1"
}
